namespace grl.flatbuffer;

// pos in meters
struct Vector3d {
  x:double;
  y:double;
  z:double;
}

// euler angles in DEGREES
struct EulerXYZd {
  rx:double;
  ry:double;
  rz:double;
}

struct Quaternion {
  x:double;
  y:double;
  z:double;
  w:double;
}

struct Frame {
  position:Vector3d;
  orientation:Quaternion;
}
